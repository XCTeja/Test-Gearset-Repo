                  <select class="subscriptionFrequencySelection" data-subscription="{{this.sku}}">
                  {{#each this.subscriptionFrequencies}}
                    <option value="{{safeQuote this}}">{{pageLabelMapMultiString 'Subscribe_And_Save_' this}}</option>
                  {{/each}}
                  </select>
                </div>
              {{/each}}

              <div class="wishFinder" data-sku="{{this.prodBean.sku}}">
                <button type="button" class="btn btn-primary slim add_to_cart_desktop_button" data-id="{{this.prodBean.id}}" data-inventory="{{this.productInventory}}" data-seller="{{this.sellerID}}">{{pageLabelMap 'Component_MiniwishList_AddToCart'}}</button>
