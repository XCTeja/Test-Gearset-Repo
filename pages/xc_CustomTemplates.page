                  {{/if}}
                  {{#ifDisplay 'PL.DsplAddtlSku'}}
                    <strong>{{pageLabelMap 'SKU'}}:</strong>&#160;{{this.prodBean.sku}}<br/><br/>
                  {{/ifDisplay}}
                  {{#ifDisplay 'PL.DsplAddtlShortDesc'}}
