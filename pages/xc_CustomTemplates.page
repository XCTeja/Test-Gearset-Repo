          {{else}}
            <div class="wishFinder" data-sku="{{this.prodBean.sku}}">
              {{#listFind this.prodBean.ProductType 'Aggregated|Dynamic Kit' false }}
              <span class="wishButtons"></span>
              {{/listFind}}
            </div>
          {{/if}}
        {{/unless}}

      </div><!--/.product-item-->
    {{/each}}
  </div><!--/.products-->
</script>
    
